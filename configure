    if test -n "$SWF_DIR"; then
      SWF_SHARED_LIBADD="-R$SWF_DIR -L$SWF_DIR -lswf $SWF_SHARED_LIBADD"
	else
      SWF_SHARED_LIBADD="-lswf $SWF_SHARED_LIBADD"
    fi
  else
